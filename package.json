    "ios": "expo run:ios"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@sentry/react-native": "^6.15.1",
    "dotenv": "^16.5.0",
    "expo": "^53.0.11",
    "expo-av": "^15.1.6",
    "logger": "^0.0.1",
    "react": "19.0.0",
    "react-native": "0.79.4"
  }
}
